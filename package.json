{
  "name": "monorepo",
  "private": true,
  "packageManager": "pnpm@8.8.0",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "eslint": "8.46.0",
    "pnpm": "8.8.0",
    "prettier": "2.8.8",
    "typescript": "5.3.2"
  },
  "scripts": {
    "bootstrap": "pnpm install",
    "dev:backend": "pnpm --filter backend dev",
    "dev:frontend": "pnpm --filter frontend dev",
    "dev": "concurrently --raw \"pnpm dev:backend\" \"pnpm dev:frontend\"",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test"
  }
}
